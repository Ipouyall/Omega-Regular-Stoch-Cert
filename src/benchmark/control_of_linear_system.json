{
  "actions": {
    "maximal_polynomial_degree": 1,
    "control_policy": []
  },

  "disturbance": {
    "distribution_name": "uniform",
    "disturbance_parameters": {
      "lower_bound": [-1],
      "upper_bound": [2]
    }
  },

  "stochastic_dynamical_system": {
    "state_space_dimension": 2,
    "control_space_dimension": 0,
    "disturbance_space_dimension": 1,
    "system_space": "S1**2 + S2**2 <= 400",
    "initial_space": "-4<=S1<=-3 and -6<=S2<=-5",
    "dynamics": [
      {
        "condition": "S1**2 + S2**2 <= 81",
        "transforms": [
          "0.2*S1 - 0.3*S2 + 0.5",
          "0.5*S1 - 0.5*S2 + 0.5"
        ]
      },
      {
        "condition": "S1**2 + S2**2 >= 81",
        "transforms": [
          "0.2*S1 - 0.3*S2 + 0.5",
          "0.5*S1 - 0.5*S2 + 0.5"
        ]
      }
    ]
  },

  "synthesis_config": {
    "maximal_polynomial_degree": 2,
    "epsilon": 0.000000000000001,
    "probability_threshold": 0.1,
    "theorem_name": "farkas",
    "solver_name": "z3",
    "owl_path": "../playground/rabinizer/owl"
  },

  "specification": {
    "ltl_formula": "F a",
    "preposition_lookup": {
      "a": "(S1-2)**2 + (S2-2.5)**2 <= 25"
    }
  }
}