    "epsilon": 0.000000000000001,
    "probability_threshold": 0.99,
    "theorem_name": "farkas",
    "solver_name": "z3",
    "owl_path": "../playground/rabinizer/owl"
  },

  "specification": {
    "ltl_formula": "GF a",
    "preposition_lookup": {
      "a": "S1 <= 50"
    }
  }
