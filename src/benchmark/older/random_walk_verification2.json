    "control_policy": []
  },

  "disturbance": {
    "distribution_name": "uniform",
    "disturbance_parameters": {
      "lower_bound": [-2],
      "upper_bound": [1]
    }
  },

  "stochastic_dynamical_system": {
    "state_space_dimension": 1,
    "control_space_dimension": 0,
